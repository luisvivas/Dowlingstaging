<h1>New Work Order</h1>

<%= form_for(@work_order) do |f| %>
  <%= f.formatted_error_messages %>
<div class="pane">
  <div class="field">
    <%= f.label :quote_id %><br />
    <%= f.hidden_field :quote_id %>
	<%= work_order_quote_autocomplete(f.object) %>
  </div>
</div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<%= render 'work_orders/collection_actions' %>